@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>签证国家管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="VisaNation.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="VisaNationTableToolbar" role="group">
                            @if(shiro.hasPermission("/visaNation/add")){
                                <#button name="添加" icon="fa-plus" clickFun="VisaNation.openAddVisaNation()"/>
                            @}
                            @if(shiro.hasPermission("/visaNation/update")){
                                <#button name="修改" icon="fa-edit" clickFun="VisaNation.openVisaNationDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/visaNation/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="VisaNation.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="VisaNationTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/travel/visaNation.js"></script>
@}
