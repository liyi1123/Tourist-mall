@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

            <input type="hidden" id="id" value="">

            <div class="row">
                <div class="col-sm-6 b-r">
                            <#input id="id" name="车辆品牌" underline="true"/>
                            <#input id="name" name="名称"/>
                </div>

                <div class="col-sm-6">

                            <#input id="carBrandName" name="所属品牌" underline="true" readonly="readonly" hidden="parentId"
                                    clickFun="CarBrandInfoDlg.showDeptSelectTree(); return false;"
                                    style="background-color: #ffffff !important;" />
                            <!-- 父级部门的选择框 -->
                            <div id="parentDeptMenu" class="menuContent" style="display: none; position: fixed; z-index: 200;">
                                <ul id="parentDeptMenuTree" class="ztree tree-box" style="width: 392px !important;"></ul>  <!--style="width: 392px !important;"-->
                            </div>

                            <#input id="comment" name="备注"  />

                </div>


            </div>



            <div class="row btn-group-m-t">
                <div class="col-sm-10">
                    <#button btnCss="info" name="提交" id="ensure" icon="fa-check" clickFun="CarBrandInfoDlg.addSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="CarBrandInfoDlg.close()"/>
                </div>
            </div>
        </div>

    </div>
</div>
<!--<script src="${ctxPath}/static/js/plugins/summernote/summernote.min.js"></script>
<script src="${ctxPath}/static/js/plugins/summernote/summernote-zh-CN.js"></script>-->
<script src="${ctxPath}/static/modular/travel/carBrand_info.js"></script>
<script type="text/javascript" src="http://tajs.qq.com/stats?sId=9051096" charset="UTF-8"></script>
@}
