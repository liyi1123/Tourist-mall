@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

            <input type="hidden" id="id" value="">

            <div class="row">
                <div class="col-sm-6 b-r">
                            <#input id="id" name="" underline="true"/>
                            <#input id="parentId" name="父ID;0-订单;大于0-订单详情" underline="true"/>
                            <#input id="orderType" name="订单类型:1景点;2酒店;3租车;4签证" underline="true"/>
                            <#input id="productId" name="对应的产品ID" underline="true"/>
                            <#input id="buyerId" name="买家ID" underline="true"/>
                            <#input id="addressId" name="收货地址表ID" underline="true"/>
                            <#input id="orderNum" name="订单编号"/>
                </div>

                <div class="col-sm-6">
                            <#input id="number" name="数量" underline="true"/>
                            <#input id="lastTime" name="最晚到店时间" underline="true"/>
                            <#input id="checkInTime" name="入住时间" underline="true"/>
                            <#input id="checkOutTime" name="离店时间" underline="true"/>
                            <#input id="totalMoney" name="总金额" underline="true"/>
                            <#input id="usingIntegral" name="使用积分" underline="true"/>
                            <#input id="desc" name="备注" underline="true"/>
                </div>
            </div>

            <div class="row btn-group-m-t">
                <div class="col-sm-10">
                    <#button btnCss="info" name="提交" id="ensure" icon="fa-check" clickFun="OrdersInfoDlg.addSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="OrdersInfoDlg.close()"/>
                </div>
            </div>
        </div>

    </div>
</div>
<!--
<script src="${ctxPath}/static/modular/travel/orders/orders_info.js"></script>
 -->
<script src="${ctxPath}/static/modular/travel/orders_info.js"></script>
@}
