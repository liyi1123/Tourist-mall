@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>租车服务管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="CarBrand.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="CarBrandTableToolbar" role="group">
                            @if(shiro.hasPermission("/carBrand/add")){
                                <#button name="添加" icon="fa-plus" clickFun="CarBrand.openAddCarBrand()"/>
                            @}
                            @if(shiro.hasPermission("/carBrand/update")){
                                <#button name="修改" icon="fa-edit" clickFun="CarBrand.openCarBrandDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/carBrand/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="CarBrand.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="CarBrandTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/travel/carBrand.js"></script>
@}
